<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/ic_news_feed_bg"

    android:orientation="vertical">

    <com.google.android.material.appbar.AppBarLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <com.google.android.material.appbar.CollapsingToolbarLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fitsSystemWindows="false"
            app:expandedTitleGravity="top"
            app:layout_scrollFlags="scroll|exitUntilCollapsed"
            app:titleEnabled="false">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:visibility="visible"
                app:layout_behavior="@string/appbar_scrolling_view_behavior">

                <LinearLayout
                    android:id="@+id/profile_image_LAY"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fitsSystemWindows="true"
                    android:orientation="vertical">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        app:layout_collapseMode="parallax">

                        <FrameLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">


                            <ImageView
                                android:id="@+id/profile_img"
                                android:layout_width="match_parent"
                                android:layout_height="300dp"
                                android:maxHeight="300dp"
                                android:scaleType="fitXY"
                                android:src="@drawable/ic_male" />

                            <View
                                android:layout_width="match_parent"
                                android:layout_height="200dp"
                                android:layout_gravity="bottom"
                                android:background="@drawable/shadow_line_image_view"
                                android:fitsSystemWindows="true" />

                            <ProgressBar
                                android:id="@+id/progress_bar"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:indeterminateTint="@color/colorPrimary"
                                android:indeterminate="true" />
                        </FrameLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_alignParentBottom="true"
                            android:layout_gravity="bottom"
                            android:orientation="horizontal"
                            android:paddingBottom="@dimen/dimen20">

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_marginStart="@dimen/dimen20"
                                android:layout_weight="1"
                                android:orientation="vertical">

                                <TextView
                                    android:id="@+id/profile_user_name"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="bottom"
                                    android:singleLine="true"
                                    android:textColor="@color/white"
                                    android:textSize="@dimen/text_20"
                                    android:textStyle="bold" />


                                <TextView
                                    android:id="@+id/lives_now"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="bottom"
                                    android:singleLine="true"
                                    android:textColor="@color/white"
                                    android:textSize="@dimen/text_14"
                                    android:textStyle="normal" />

                            </LinearLayout>


                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:layout_gravity="center|left"
                                android:layout_marginRight="15dp"
                                android:orientation="horizontal">

                                <View

                                    android:id="@+id/profile_edit_icon_view"
                                    android:layout_width="2dp"
                                    android:layout_height="@dimen/dimen_35"
                                    android:layout_gravity="center"
                                    android:layout_marginLeft="@dimen/dimen10"
                                    android:background="@android:color/white"
                                    android:visibility="gone" />

                                <ImageView
                                    android:id="@+id/profile_edit_icon"
                                    android:layout_width="30dp"
                                    android:layout_height="30dp"
                                    android:layout_gravity="center|left"
                                    android:layout_marginLeft="@dimen/dimen10"
                                    android:fitsSystemWindows="true"
                                    android:scaleType="fitCenter"
                                    android:background="@drawable/ic_upload"
                                    android:backgroundTint="@color/white"
                                    android:visibility="gone" />

                            </LinearLayout>

                        </LinearLayout>


                        <androidx.appcompat.widget.Toolbar
                            android:id="@+id/toolbar2"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="10dp"
                            android:layout_marginTop="@dimen/dimen10"
                            app:contentInsetLeft="@dimen/dimen_0"
                            app:contentInsetStart="@dimen/dimen_0">

                            <ImageView
                                android:id="@+id/back_icon"
                                android:layout_width="36dp"
                                android:layout_height="36dp"
                                android:background="@drawable/profile_back_btn"
                                android:padding="2.5dp"
                                app:tint="@color/white" />
                        </androidx.appcompat.widget.Toolbar>


                    </RelativeLayout>


                </LinearLayout>

                <LinearLayout
                    android:id="@+id/fragment_profile_add_LAY"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/frameLayout"
                    android:background="@color/profile_friends_follow_label_bg"
                    android:orientation="horizontal"
                    android:padding="@dimen/dimen_18"
                    android:visibility="visible">


                    <TextView
                        android:id="@+id/add_friends_TET"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:singleLine="true"
                        android:text="@string/label_add_friends"
                        android:textColor="@android:color/black"
                        android:textSize="@dimen/text_12"
                        android:textStyle="bold" />


                    <View
                        android:layout_width="1dp"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="@dimen/dimen10"
                        android:layout_marginTop="-10dp"
                        android:layout_marginRight="@dimen/dimen10"
                        android:layout_marginBottom="-10dp"
                        android:background="@color/profile_friends_follow_label_line_bg" />


                    <TextView
                        android:id="@+id/add_follow_TET"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_weight="0.75"
                        android:gravity="center"
                        android:singleLine="true"
                        android:text="@string/label_follow"
                        android:textColor="@color/black"
                        android:textSize="@dimen/text_12"
                        android:textStyle="bold" />

                    <View
                        android:layout_width="1dp"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="@dimen/dimen10"
                        android:layout_marginRight="@dimen/dimen10"
                        android:background="@color/profile_friends_follow_label_line_bg" />


                    <TextView
                        android:id="@+id/add_message_TET"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_weight="0.75"
                        android:gravity="center"
                        android:singleLine="true"
                        android:text="@string/label_message"
                        android:textColor="@color/black"
                        android:textSize="@dimen/text_12"
                        android:textStyle="bold" />


                    <View
                        android:layout_width="1dp"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="@dimen/dimen10"
                        android:layout_marginRight="@dimen/dimen10"
                        android:background="@color/profile_friends_follow_label_line_bg" />


                    <TextView
                        android:id="@+id/add_more_TET"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_weight="0.75"
                        android:gravity="center"
                        android:singleLine="true"
                        android:text="@string/label_more_small"
                        android:textAllCaps="false"
                        android:textColor="@color/black"
                        android:textSize="@dimen/text_12"
                        android:textStyle="bold" />

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/Total_LAY"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/white"
                    android:orientation="horizontal"
                    android:visibility="gone"
                    android:weightSum="4">

                    <LinearLayout
                        android:id="@+id/fragment_profile_total_photos_LAY"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:weightSum="2">

                        <TextView
                            android:id="@+id/fragment_profile_total_photos"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_margin="5dp"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:text=""
                            android:textColor="@color/black"
                            android:textSize="@dimen/text_16" />

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="5dp"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:text="@string/label_photos"
                            android:textColor="@color/bg_light_grey"
                            android:textSize="@dimen/text_12" />

                    </LinearLayout>

                    <View
                        android:layout_width="1dp"
                        android:layout_height="match_parent"
                        android:layout_marginTop="@dimen/dimen10"
                        android:layout_marginBottom="@dimen/dimen10"
                        android:background="@color/dark_grey" />

                    <LinearLayout
                        android:id="@+id/fragment_profile_total_friends_LAY"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:weightSum="2">

                        <TextView
                            android:id="@+id/fragment_profile_total_friends"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_margin="5dp"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:text=""
                            android:textColor="@color/black"
                            android:textSize="@dimen/text_16"
                            android:textStyle="bold" />

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="5dp"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:text="@string/label_friend"
                            android:textColor="@color/bg_light_grey"
                            android:textSize="@dimen/text_12"
                            android:textStyle="bold" />

                    </LinearLayout>

                    <View

                        android:layout_width="1dp"
                        android:layout_height="match_parent"

                        android:background="@color/blue" />


                    <LinearLayout
                        android:id="@+id/fragment_profile_total_followers_LAY"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:weightSum="2">

                        <TextView
                            android:id="@+id/fragment_profile_total_followers"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_margin="5dp"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:text=""

                            android:textColor="@color/black"
                            android:textSize="@dimen/text_16"
                            android:textStyle="bold" />

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="5dp"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:text="@string/label_followers"
                            android:textColor="@color/bg_light_grey"
                            android:textSize="@dimen/text_12"
                            android:textStyle="bold" />

                    </LinearLayout>

                    <View
                        android:layout_width="1dp"
                        android:layout_height="match_parent"
                        android:background="@color/blue" />

                    <LinearLayout
                        android:id="@+id/fragment_profile_total_following_LAY"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:weightSum="2">

                        <TextView
                            android:id="@+id/fragment_profile_total_following"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_margin="5dp"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:text=""
                            android:textColor="@color/black"
                            android:textSize="@dimen/text_16"
                            android:textStyle="bold" />

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="5dp"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:text="@string/label_following"
                            android:textColor="@color/bg_light_grey"
                            android:textSize="@dimen/text_12"
                            android:textStyle="bold" />

                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/Status_LAY"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/Total_LAY"
                    android:background="@color/white"
                    android:orientation="vertical"
                    android:weightSum="4">

                    <LinearLayout
                        android:id="@+id/fragment_profile_status_LAY"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="25dp"
                        android:layout_marginTop="5dp"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:weightSum="2">

                        <ImageView
                            android:layout_width="80dp"
                            android:layout_height="20dp"
                            android:layout_marginTop="5dp"
                            android:layout_weight="1"
                            android:background="@drawable/profile_status"
                            android:textSize="@dimen/text_18"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/status_tv"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/dimen_1"
                            android:layout_marginTop="5dp"
                            android:layout_marginBottom="@dimen/dimen5"
                            android:layout_weight="1"
                            android:textColor="@color/profile_status_description_text_color"
                            android:textSize="@dimen/text_14" />

                    </LinearLayout>

                    <View
                        android:layout_width="30dp"
                        android:layout_height="3dp"
                        android:layout_marginStart="25dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginEnd="20dp"
                        android:layout_marginBottom="5dp"
                        android:background="@color/profile_status_label_line_bg" />

                    <LinearLayout
                        android:id="@+id/fragment_profile_profession_LAY"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="25dp"
                        android:layout_marginTop="5dp"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:weightSum="2">

                        <ImageView
                            android:layout_width="135dp"
                            android:layout_height="25dp"
                            android:layout_marginTop="5dp"
                            android:layout_weight="1"
                            android:background="@drawable/profile_profession_title_image"
                            android:textColor="@color/blue"
                            android:textSize="@dimen/text_18"
                            android:textStyle="bold" />


                        <TextView
                            android:id="@+id/profession_tv"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/dimen_1"
                            android:layout_marginTop="5dp"
                            android:layout_marginBottom="@dimen/dimen5"
                            android:layout_weight="1"
                            android:textColor="@color/profile_status_description_text_color"
                            android:textSize="@dimen/text_14" />

                    </LinearLayout>

                    <View
                        android:layout_width="30dp"
                        android:layout_height="3dp"
                        android:layout_marginStart="25dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginEnd="20dp"
                        android:layout_marginBottom="5dp"
                        android:background="@color/profile_status_label_line_bg" />


                    <LinearLayout
                        android:id="@+id/fragment_profile_education_LAY"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="25dp"
                        android:layout_marginTop="5dp"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:weightSum="2">

                        <ImageView
                            android:layout_width="135dp"
                            android:layout_height="21dp"
                            android:layout_marginTop="5dp"
                            android:layout_weight="1"
                            android:background="@drawable/profile_education_title_img"
                            android:text="@string/label_education"
                            android:textColor="@color/blue"
                            android:textSize="@dimen/text_18"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/education_tv"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/dimen_1"
                            android:layout_marginTop="5dp"
                            android:layout_marginBottom="@dimen/dimen5"
                            android:layout_weight="1"
                            android:textColor="@color/profile_status_description_text_color"
                            android:textSize="@dimen/text_14" />

                    </LinearLayout>

                    <View
                        android:layout_width="30dp"
                        android:layout_height="3dp"
                        android:layout_marginStart="25dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginEnd="20dp"
                        android:layout_marginBottom="5dp"
                        android:background="@color/profile_status_label_line_bg" />

                    <LinearLayout
                        android:id="@+id/fragment_profile_hometown_LAY"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="25dp"
                        android:layout_marginTop="5dp"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:weightSum="2">

                        <ImageView
                            android:layout_width="135dp"
                            android:layout_height="21dp"
                            android:layout_marginTop="5dp"
                            android:layout_weight="1"
                            android:background="@drawable/profile_hometown_title_img"
                            android:textColor="@color/blue"
                            android:textSize="@dimen/text_14"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/hometown_tv"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/dimen_1"
                            android:layout_marginTop="5dp"
                            android:layout_marginBottom="5dp"
                            android:layout_weight="1"
                            android:textColor="@color/profile_status_description_text_color"
                            android:textSize="@dimen/text_14" />

                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginLeft="@dimen/dimen20"
                        android:layout_marginTop="@dimen/dimen10"
                        android:layout_marginRight="@dimen/dimen20"
                        android:layout_marginBottom="@dimen/dimen10"
                        android:background="@color/medium_grey" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/white"
                    android:orientation="vertical"
                    android:paddingLeft="@dimen/dimen20"
                    android:paddingRight="@dimen/dimen20">


                    <RadioGroup
                        android:id="@+id/radio_group"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dimen10"
                        android:layout_marginBottom="@dimen/dimen20"
                        android:layout_weight="3"
                        android:orientation="horizontal">


                        <RadioButton
                            android:id="@+id/profile_screen_friends_rd_button"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_marginStart="@dimen/dimen10"
                            android:layout_marginEnd="@dimen/dimen5"
                            android:layout_weight="1"
                            android:background="@drawable/tab_radio_selector_layout"
                            android:button="@android:color/transparent"
                            android:checked="true"
                            android:gravity="center"
                            android:padding="@dimen/dimen_8"
                            android:singleLine="true"
                            android:text="@string/label_friends"
                            android:textColor="@color/white"
                            android:textSize="@dimen/text_12" />

                        <RadioButton
                            android:id="@+id/profile_screen_follower_rd_button"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/dimen5"
                            android:layout_marginRight="@dimen/dimen5"
                            android:layout_weight="1"
                            android:background="@drawable/tab_radio_selector_layout"
                            android:button="@android:color/transparent"
                            android:gravity="center"
                            android:padding="@dimen/dimen_8"
                            android:singleLine="true"
                            android:text="@string/label_followers"
                            android:textColor="@color/black"
                            android:textSize="@dimen/text_12" />


                        <RadioButton
                            android:id="@+id/profile_screen_following_rd_button"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/dimen5"
                            android:layout_marginEnd="@dimen/dimen10"
                            android:layout_weight="1"
                            android:background="@drawable/tab_radio_selector_layout"
                            android:button="@android:color/transparent"
                            android:gravity="center"
                            android:padding="@dimen/dimen_8"
                            android:singleLine="true"
                            android:text="@string/label_following"
                            android:textColor="@color/black"
                            android:textSize="@dimen/text_12" />

                    </RadioGroup>

                    <androidx.recyclerview.widget.RecyclerView
                        android:id="@+id/friendsRV"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />


                    <TextView
                        android:id="@+id/profile_screen_empty_friends_follow_list"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center|right"
                        android:layout_marginTop="10dp"
                        android:layout_marginEnd="@dimen/dimen5"
                        android:layout_marginBottom="5dp"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="@string/no_one_yet"
                        android:textColor="@color/dark_grey"
                        android:textSize="@dimen/text_16"
                        android:visibility="gone" />

                    <TextView
                        android:id="@+id/profile_screen_dynamic_more_list_TET"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center|right"
                        android:layout_marginTop="20dp"
                        android:layout_marginEnd="@dimen/dimen5"
                        android:layout_marginBottom="15dp"
                        android:layout_weight="1"
                        android:gravity="right"
                        android:text="See all"
                        android:textColor="@color/dark_grey"
                        android:textSize="@dimen/text_16"
                        android:visibility="gone" />


                </LinearLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/white">

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_margin="@dimen/dimen20"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="5dp"
                        android:background="@color/medium_grey" />
                    <!--profile Screen dynamic view-->

                </RelativeLayout>

                <LinearLayout
                    android:id="@+id/profile_screen_dynamic_profile_image_LAY"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/white"
                    android:orientation="vertical"
                    android:paddingLeft="@dimen/dimen20"
                    android:paddingRight="@dimen/dimen20"
                    android:visibility="gone">


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center|left"
                            android:layout_marginStart="@dimen/dimen5"
                            android:layout_marginTop="10dp"
                            android:layout_marginBottom="5dp"
                            android:layout_weight="1"
                            android:gravity="left|center"
                            android:text="@string/label_photos"
                            android:textColor="@color/black"
                            android:textSize="@dimen/text_14"
                            android:visibility="visible" />

                        <TextView
                            android:id="@+id/profile_screen_photo_count"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center|right"
                            android:layout_marginTop="10dp"
                            android:layout_marginEnd="@dimen/dimen5"
                            android:layout_marginBottom="5dp"
                            android:layout_weight="1"
                            android:gravity="right|center"
                            android:text=""
                            android:textColor="@color/black"
                            android:textSize="@dimen/text_16"
                            android:visibility="visible" />

                    </LinearLayout>

                    <androidx.recyclerview.widget.RecyclerView
                        android:id="@+id/images_rv"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dimen5"
                        android:orientation="horizontal" />

                    <LinearLayout
                        android:id="@+id/dynamic_profile_image_LAY"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:visibility="gone"></LinearLayout>

                    <TextView
                        android:id="@+id/profile_screen_dynamic_profile_more_list_TET"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center|right"
                        android:layout_marginTop="10dp"
                        android:layout_marginEnd="@dimen/dimen5"
                        android:layout_marginBottom="5dp"
                        android:layout_weight="1"
                        android:gravity="right"
                        android:padding="20dp"
                        android:text="See all"
                        android:textColor="@color/dark_grey"
                        android:textSize="@dimen/text_16"
                        android:visibility="gone" />

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/history_LAY"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/profile_screen_dynamic_profile_image_LAY"
                    android:background="@color/profile_friends_follow_label_bg"
                    android:orientation="vertical"
                    android:padding="@dimen/dimen_25"
                    android:visibility="gone">

                    <ImageView
                        android:layout_width="120dp"
                        android:layout_height="21dp"
                        android:layout_gravity="center"
                        android:background="@drawable/history_label_img"
                        android:gravity="center"
                        android:padding="@dimen/dimen5" />

                </LinearLayout>

            </LinearLayout>

        </com.google.android.material.appbar.CollapsingToolbarLayout>


    </com.google.android.material.appbar.AppBarLayout>

    <com.truelife.app.VideoPlayerRecyclerView
        android:id="@+id/publicRv"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:nestedScrollingEnabled="true"
        app:layout_behavior="@string/appbar_scrolling_view_behavior" />

</androidx.coordinatorlayout.widget.CoordinatorLayout>